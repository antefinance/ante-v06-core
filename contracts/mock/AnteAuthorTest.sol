import "../AnteTest.sol";

/// Ante test setting the test author in constructor
contract AnteAuthorTest is AnteTest("I can set testAuthor") {
    constructor(address _testAuthor) {
        if (_testAuthor != address(0)) {
            testAuthor = _testAuthor;
        }
    }

    function checkTestPasses() public pure override returns (bool) {
        return true;
    }
